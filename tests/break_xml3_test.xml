<?xml version="1.0"?>
<!DOCTYPE OpenCReport SYSTEM "opencreport.dtd">
<OpenCReport>
	<Datasources>
		<Datasource name="myarray" type="'array'" />
	</Datasources>
	<Queries>
		<Query name="a" datasource="myarray" value="'array'" coltypes="'coltypes'" rows="5" cols="5" />
	</Queries>
	<Report>
		<Breaks>
			<Break name="adult">
				<BreakFields>
					<BreakField value="age > 18" />
				</BreakFields>
			</Break>
		</Breaks>
	</Report>
</OpenCReport>
